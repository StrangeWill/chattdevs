<%= form_for @job do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :company_name %>
  <%= f.text_field :company_name %>
  <%= f.label :company_website %>
  <%= f.text_field :company_website %>
  <%= f.label :how_to_apply %>
  <%= f.text_field :how_to_apply %>
  <%= f.submit %>
<% end %>